	<arg:Model class="BarkerERShapeDomainModel" enableDiagramRules="true">
		<arg:RuleContainer class="BarkerERDiagram">
			<arg:DeletingRule methodName="ConceptTypeDetachingFromEntityTypeRule">
				<arg:RuleOn targetType="EntityTypeIsPrimarilyForConceptType" targetTypeQualifier="Neumont.Tools.ORMAbstractionToBarkerERBridge"/>
			</arg:DeletingRule>
			<arg:DeletingRule methodName="ConceptTypeDetachingFromObjectTypeRule">
				<arg:RuleOn targetType="ConceptTypeIsForObjectType" targetTypeQualifier="Neumont.Tools.ORMToORMAbstractionBridge"/>
			</arg:DeletingRule>
			<arg:ChangeRule methodName="DisplayAttributePropertyChangedRule">
				<arg:RuleOn targetType="Attribute" targetTypeQualifier="Neumont.Tools.EntityRelationshipModels.Barker"/>
			</arg:ChangeRule>
			<arg:ChangeRule methodName="DisplayEntityTypePropertyChangedRule">
				<arg:RuleOn targetType="EntityType" targetTypeQualifier="Neumont.Tools.EntityRelationshipModels.Barker"/>
			</arg:ChangeRule>
			<arg:ChangeRule methodName="NameChangedRule">
				<arg:RuleOn targetType="BarkerERDiagram"/>
			</arg:ChangeRule>
			<arg:AddRule methodName="BinaryAssociationAddedRule" alwaysOn="true">
				<arg:RuleOn targetType="BarkerErModelContainsBinaryAssociation" targetTypeQualifier="Neumont.Tools.EntityRelationshipModels.Barker" fireTime="TopLevelCommit" priority="DiagramFixupConstants.AddConnectionRulePriority"/>
			</arg:AddRule>
		</arg:RuleContainer>
		<!-- This is not really a rule, but an element that we want to add to the directory. -->
		<arg:ReflectedType class="AttributeElementListCompartment"/>
	</arg:Model>
</arg:Rules>
