<?xml version="1.0" encoding="utf-8" ?>
<root>

	<entityType name="Person">
		<valueType name="firstName"/>
		<valueType name="lastName"/>
		<valueType name="gender"/>
		<valueType name="isFemale"/>
		<valueType name="pregnancyCount">
			<constraint type="subset" target="isFemale"/>
		</valueType>
	</entityType>



	<entityType name="Person">
		<valueType name="firstName"/>
		<valueType name="lastName"/>
		<valueType name="gender"/>
		<unique value="firstName,lastName"/>
	</entityType>
	<entityType name="Woman">
		<valueType name="firstName"/>
		<valueType name="lastName"/>
		<constraint name="(firstName,lastName) references Person(firstName,lastName) where gender='F'"/>
		<unique value="firstName,lastName"/>
	</entityType>

	<entityType name="Enrollment">
		<entityType ref="Person"/>
		<entityType ref="Course"/>
		<unique value="Person,Course"/>
	</entityType>

	
	
	
	
	
</root>