<?xml version="1.0" encoding="utf-8" ?>
<ds:settings xmlns:ds="http://schemas.neumont.edu/ORM/DesignerSettings">
	<ds:xmlConverters xmlns:unversionedOrmRoot="http://schemas.neumont.edu/ORM/ORMRoot" xmlns:unversionedOrmCore="http://schemas.neumont.edu/ORM/ORMCore">
		<ds:xmlConverter description="Import standalone core model file" sourceElement=".ORMModel" targetElement=".ORMRoot" transformFile="CoreModelImport.xslt"/>
		<ds:xmlConverter description="Import Visio ORM models from Orthogonal Toolbox" sourceElement="VisioModels" targetElement="unversionedOrmCore:ORMModel" transformFile="VisioToCoreModelImport.xslt"/>
		<ds:xmlConverter description="Upgrade core model to 2006-01 schema" sourceElement="unversionedOrmCore:ORMModel" targetElement=".ORMModel" transformFile="CoreUpgradeTo2006-01.xslt"/>
		<ds:xmlConverter description="Upgrade root model to 2006-01 schema" sourceElement="unversionedOrmRoot:ORM2" targetElement=".ORMRoot" transformFile="RootUpgradeTo2006-01.xslt"/>
	</ds:xmlConverters>
</ds:settings>