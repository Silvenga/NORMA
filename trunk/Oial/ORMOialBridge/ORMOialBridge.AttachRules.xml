	<arg:Model class="ORMToORMAbstractionBridgeDomainModel">
		<arg:RuleContainer class="AbstractionModelIsForORMModel.ORMElementGateway">
			<arg:AddRule methodName="ElementExclusionAddedRule">
				<arg:RuleOn targetType="ExcludedORMModelElement"/>
			</arg:AddRule>
			<arg:AddRule methodName="FactTypeAddedRule">
				<arg:RuleOn targetType="ModelHasFactType" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:AddRule>
			<arg:AddRule methodName="FactTypeDerivationAddedRule">
				<arg:RuleOn targetType="FactTypeHasDerivationExpression" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:AddRule>
			<arg:ChangeRule methodName="FactTypeDerivationChangedRule">
				<arg:RuleOn targetType="FactTypeDerivationExpression" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:ChangeRule>
			<arg:DeleteRule methodName="FactTypeDerivationDeletedRule">
				<arg:RuleOn targetType="FactTypeHasDerivationExpression" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:DeleteRule>
			<arg:AddRule methodName="FactTypeErrorAddedRule">
				<arg:RuleOn targetType="FactTypeHasImpliedInternalUniquenessConstraintError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
				<arg:RuleOn targetType="FactTypeHasFactTypeRequiresInternalUniquenessConstraintError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:AddRule>
			<arg:DeleteRule methodName="FactTypeErrorDeletedRule">
				<arg:RuleOn targetType="FactTypeHasImpliedInternalUniquenessConstraintError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
				<arg:RuleOn targetType="FactTypeHasFactTypeRequiresInternalUniquenessConstraintError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:DeleteRule>
			<arg:AddRule methodName="ObjectTypeAddedRule">
				<arg:RuleOn targetType="ModelHasObjectType" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:AddRule>
			<arg:AddRule methodName="ObjectTypeErrorAddedRule">
				<arg:RuleOn targetType="ObjectTypeHasEntityTypeRequiresReferenceSchemeError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
				<arg:RuleOn targetType="ObjectTypeHasCompatibleSupertypesError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
				<arg:RuleOn targetType="ObjectTypeHasPreferredIdentifierRequiresMandatoryError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
				<arg:RuleOn targetType="ValueTypeHasUnspecifiedDataTypeError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:AddRule>
			<arg:DeleteRule methodName="ObjectTypeErrorDeletedRule">
				<arg:RuleOn targetType="ObjectTypeHasEntityTypeRequiresReferenceSchemeError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
				<arg:RuleOn targetType="ObjectTypeHasCompatibleSupertypesError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
				<arg:RuleOn targetType="ObjectTypeHasPreferredIdentifierRequiresMandatoryError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
				<arg:RuleOn targetType="ValueTypeHasUnspecifiedDataTypeError" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:DeleteRule>
			<arg:AddRule methodName="ObjectificationAddedRule">
				<arg:RuleOn targetType="Objectification" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:AddRule>
			<arg:DeleteRule methodName="ObjectificationDeletedRule">
				<arg:RuleOn targetType="Objectification" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:DeleteRule>
			<arg:RolePlayerChangeRule methodName="ObjectificationRolePlayerChangedRule">
				<arg:RuleOn targetType="Objectification" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:RolePlayerChangeRule>
			<arg:AddRule methodName="PreferredIdentifierAddedRule">
				<arg:RuleOn targetType="EntityTypeHasPreferredIdentifier" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:AddRule>
			<arg:AddRule methodName="RolePlayerAddedRule">
				<arg:RuleOn targetType="ObjectTypePlaysRole" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:AddRule>
			<arg:DeleteRule methodName="RolePlayerDeletedRule">
				<arg:RuleOn targetType="ObjectTypePlaysRole" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:DeleteRule>
			<arg:RolePlayerChangeRule methodName="RolePlayerRolePlayerChangedRule">
				<arg:RuleOn targetType="ObjectTypePlaysRole" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:RolePlayerChangeRule>
		</arg:RuleContainer>
		<arg:RuleContainer class="AbstractionModelIsForORMModel.ModificationTracker">
			<arg:DeleteRule methodName="ConceptTypeBridgeDetachedRule">
				<arg:RuleOn targetType="ConceptTypeIsForObjectType"/>
			</arg:DeleteRule>
			<arg:DeleteRule methodName="InformationTypeFormatBridgeDetachedRule">
				<arg:RuleOn targetType="InformationTypeFormatIsForValueType"/>
			</arg:DeleteRule>
			<arg:DeleteRule methodName="ConceptTypeChildPathBridgeDetachedRule">
				<arg:RuleOn targetType="ConceptTypeChildHasPathFactType"/>
			</arg:DeleteRule>
			<arg:AddRule methodName="ConstraintRoleAddedRule">
				<arg:RuleOn targetType="ConstraintRoleSequenceHasRole" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:AddRule>
			<arg:DeleteRule methodName="ConstraintRoleDeletedRule">
				<arg:RuleOn targetType="ConstraintRoleSequenceHasRole" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:DeleteRule>
			<arg:ChangeRule methodName="ObjectTypeChangedRule">
				<arg:RuleOn targetType="ObjectType" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:ChangeRule>
			<arg:ChangeRule methodName="ORMModelChangedRule">
				<arg:RuleOn targetType="ORMModel" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:ChangeRule>
			<arg:DeleteRule methodName="PreferredIdentifierDeletedRule">
				<arg:RuleOn targetType="EntityTypeHasPreferredIdentifier" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:DeleteRule>
			<arg:RolePlayerChangeRule methodName="PreferredIdentifierRolePlayerChangedRule">
				<arg:RuleOn targetType="EntityTypeHasPreferredIdentifier" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:RolePlayerChangeRule>
			<arg:RolePlayerChangeRule methodName="RolePlayerRolePlayerChangedRule">
				<arg:RuleOn targetType="ObjectTypePlaysRole" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:RolePlayerChangeRule>
			<arg:ChangeRule methodName="SetConstraintChangedRule">
				<arg:RuleOn targetType="SetConstraint" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:ChangeRule>
			<arg:ChangeRule methodName="SubtypeFactChangedRule">
				<arg:RuleOn targetType="SubtypeFact" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:ChangeRule>
			<arg:RolePlayerPositionChangeRule methodName="UniquenessConstraintRoleOrderChanged">
				<arg:RuleOn targetType="ConstraintRoleSequenceHasRole" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:RolePlayerPositionChangeRule>
			<arg:DeletingRule methodName="UniquenessConstraintRoleDeleting">
				<arg:RuleOn targetType="ConstraintRoleSequenceHasRole" targetTypeQualifier="Neumont.Tools.ORM.ObjectModel"/>
			</arg:DeletingRule>
			<arg:DeleteRule methodName="UniquenessBridgeDetachedRule">
				<arg:RuleOn targetType="UniquenessIsForUniquenessConstraint"/>
			</arg:DeleteRule>
		</arg:RuleContainer>
	</arg:Model>
</arg:Rules>