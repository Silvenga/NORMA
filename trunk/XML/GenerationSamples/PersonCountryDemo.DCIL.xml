<?xml version="1.0" encoding="utf-8"?>
<dcl:schema name="PersonCountryDemo" xmlns:dcl="http://schemas.orm.net/DIL/DCIL" xmlns:dil="http://schemas.orm.net/DIL/DIL" xmlns:ddt="http://schemas.orm.net/DIL/DILDT" xmlns:dep="http://schemas.orm.net/DIL/DILEP" xmlns:dml="http://schemas.orm.net/DIL/DMIL" xmlns:ddl="http://schemas.orm.net/DIL/DDIL">
	<dcl:domainDataType name="Title" oilRefName="Title">
		<dcl:predefinedDataType name="CHARACTER VARYING" length="4" />
		<dcl:checkConstraint name="Title_Chk">
			<dep:inPredicate type="IN">
				<dep:valueKeyword />
				<ddt:characterStringLiteral value="Dr" />
				<ddt:characterStringLiteral value="Prof" />
				<ddt:characterStringLiteral value="Mr" />
				<ddt:characterStringLiteral value="Mrs" />
				<ddt:characterStringLiteral value="Miss" />
				<ddt:characterStringLiteral value="Ms" />
			</dep:inPredicate>
		</dcl:checkConstraint>
	</dcl:domainDataType>
	<dcl:domainDataType name="Region_code" oilRefName="Region_code">
		<dcl:predefinedDataType name="CHARACTER" length="8" />
		<dcl:checkConstraint name="Region_code_Chk">
			<dep:comparisonPredicate operator="greaterThanOrEquals">
				<dep:parenthesizedValueExpression>
					<dep:charLengthExpression>
						<dep:trimFunction specification="BOTH">
							<dep:trimSource>
								<dep:valueKeyword />
							</dep:trimSource>
						</dep:trimFunction>
					</dep:charLengthExpression>
				</dep:parenthesizedValueExpression>
				<ddt:exactNumericLiteral value="8" />
			</dep:comparisonPredicate>
		</dcl:checkConstraint>
	</dcl:domainDataType>
	<dcl:table name="Person" oilRefName="Person">
		<dcl:column name="Person_id" isNullable="false" isIdentity="true" oilRefName="Person_id">
			<dcl:predefinedDataType name="BIGINT" />
		</dcl:column>
		<dcl:uniquenessConstraint name="InternalUniquenessConstraint1" isPrimary="true" oilRefName="InternalUniquenessConstraint1">
			<dcl:columnRef name="Person_id" oilRefName="Person_id" />
		</dcl:uniquenessConstraint>
		<dcl:column name="LastName" isNullable="false" isIdentity="false" oilRefName="LastName">
			<dcl:predefinedDataType name="CHARACTER VARYING" length="30" />
		</dcl:column>
		<dcl:column name="FirstName" isNullable="false" isIdentity="false" oilRefName="FirstName">
			<dcl:predefinedDataType name="CHARACTER VARYING" length="30" />
		</dcl:column>
		<dcl:column name="Title" isNullable="true" isIdentity="false" oilRefName="Title">
			<dcl:domainDataTypeRef name="Title" />
		</dcl:column>
		<dcl:column isNullable="true" isIdentity="false" oilRefName="Country_name" name="Country_Country_name">
			<dcl:predefinedDataType name="CHARACTER VARYING" length="20" />
		</dcl:column>
		<dcl:referenceConstraint name="Country_FK" oilRefName="Country" targetTable="Country">
			<dcl:columnRef sourceName="Country_Country_name" targetName="Country_name" oilRefName="Country_Country_name" />
		</dcl:referenceConstraint>
	</dcl:table>
	<dcl:table name="Country" oilRefName="Country">
		<dcl:column name="Country_name" isNullable="false" isIdentity="false" oilRefName="Country_name">
			<dcl:predefinedDataType name="CHARACTER VARYING" length="20" />
		</dcl:column>
		<dcl:uniquenessConstraint name="InternalUniquenessConstraint3" isPrimary="true" oilRefName="InternalUniquenessConstraint3">
			<dcl:columnRef name="Country_name" oilRefName="Country_name" />
		</dcl:uniquenessConstraint>
		<dcl:column name="Region_Region_code" isNullable="true" isIdentity="false" oilRefName="Region_Region_code">
			<dcl:domainDataTypeRef name="Region_code" />
		</dcl:column>
	</dcl:table>
</dcl:schema>