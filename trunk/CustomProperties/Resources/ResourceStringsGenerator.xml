	<xs:schema attributeFormDefault="unqualified" elementFormDefault="unqualified">
		<xs:element name="ResourceStrings">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="Copyright" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Copyright notice.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="CopyrightLine" type="xs:string" maxOccurs="unbounded"/>
							</xs:sequence>
							<xs:attribute name="name" type="xs:string" use="required"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="ResourceString" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="comment" maxOccurs="unbounded">
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="xs:string">
												<xs:attribute name="type" use="required">
													<xs:simpleType>
														<xs:union memberTypes="xs:NCName">
															<xs:simpleType>
																<xs:restriction base="xs:NCName">
																	<xs:enumeration value="summary"/>
																	<xs:enumeration value="remarks"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:union>
													</xs:simpleType>
												</xs:attribute>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="name" type="xs:NCName" use="required"/>
							<xs:attribute name="resourceName" type="xs:token" use="required"/>
							<xs:attribute name="model" use="required">
								<xs:simpleType>
									<xs:restriction base="xs:NCName">
										<xs:enumeration value="CustomProperties"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
	</xs:schema>
	<ResourceString name="CustomPropertiesSnippetsTypeDescription" model="CustomProperties" resourceName="Verbalization.CustomPropertiesSnippetsTypeDescription">
		<comment type="summary">The description for the custom properties verbalization snippets associated with the custom properties model.</comment>
	</ResourceString>
	<ResourceString name="CustomPropertiesSnippetsDefaultDescription" model="CustomProperties" resourceName="Verbalization.CustomPropertiesSnippetsDefaultDescription">
		<comment type="summary">The description for the default custom properties verbalization snippets for the custom properties model.</comment>
	</ResourceString>
</ResourceStrings>
